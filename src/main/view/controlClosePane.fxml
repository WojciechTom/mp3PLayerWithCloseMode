<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.ControlClosePaneController">
   <children>
      <VBox AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <Label text="Ustaw czas">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <HBox>
               <children>
                  <Slider fx:id="TimeMinuteSlider" max="59.0" orientation="VERTICAL">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </Slider>
               </children>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
            <Label fx:id="TimeMinuteLabel" alignment="CENTER" contentDisplay="RIGHT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="37.0" prefWidth="82.0" text="25">
               <padding>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </padding>
            </Label>
            <AnchorPane />
            <Button fx:id="StartButton" mnemonicParsing="false" onAction="#startStopAction" text="START">
               <VBox.margin>
                  <Insets bottom="10.0" left="15.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
